	<modules>

		<module name="ChatModule" url="modules/ChatModule.swf" 
			uri="rtmp://192.168.0.230/bigbluebutton" 
			dependsOn="ViewersModule"	
			translationOn="true"
			translationEnabled="true"	 
		/>
		
		<module name="ViewersModule" url="modules/ViewersModule.swf" 
			uri="rtmp://192.168.0.230/bigbluebutton" 
			host="http://192.168.0.230/bigbluebutton/api/enter"
