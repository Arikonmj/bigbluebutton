			uri="rtmp://192.168.0.230/deskShare"
		/>
		
		<module name="PhoneModule" url="modules/PhoneModule.swf" 
			uri="rtmp://192.168.0.230/sip" 
			autoJoin="false"
			dependsOn="ViewersModule"
		/>
		
		<module name="VideoconfModule" url="modules/VideoconfModule.swf" 
			uri="rtmp://192.168.0.230/video"
			dependsOn="ViewersModule"
			videoQuality="70"
			presenterShareOnly="false"
			resolutions="160x120,320x240,640x480,1280x720"
		/>
		
		<module name="WhiteboardModule" url="modules/WhiteboardModule.swf" 
			uri="rtmp://192.168.0.230/bigbluebutton" 
			dependsOn="PresentModule"
		/>
		
		<module name="PresentModule" url="modules/PresentModule.swf" 
			uri="rtmp://192.168.0.230/bigbluebutton" 
			host="http://192.168.0.230" 
			dependsOn="ViewersModule"
		/>

